<script type="text/javascript">
    $(function(){
        $('#nav_menu .show-list').on("mouseenter", function () {
            var slide = $('#nav-slide');
            var slider = $('#nav-slider');
            var r = $(this).parent();
            var s = r.index() - 1;
            var q = r.find("ul").clone(true);
            var o = r.width();
            var g = r.find("ul").length > 0;
            if (slide.is(":visible")) {
                g ? slide.html(q.show()).show() : slide.hide();
                slide.stop(true, false).animate({
                    height: q.height(),
                    left: 96 + (o * s)
                }, 300);
            } else {
                g ? slide.html(q.show()).show() : slide.hide();
                slide.css({
                    left: 96 + (o * s)
                }).stop(true, false).animate({
                    height: q.height()
                }, 300);
            }
            if (slider.is(":visible")) {
                slider.show().stop(true, false).animate({
                    left: 96 + (o * s)
                }, 300);
            } else {
                slider.show().css({
                    left: 96 + (o * s)
                });
            }
        });
        $('#nav-slide').on("mouseenter", function () {
            $(this).show().stop(true, false).animate({
                height: $(this).height()
            }, 200);
            $('#nav-slider').show()
        });
        $("#nav_menu .show-list,#nav-slide").on("mouseleave", function () {
            $('#nav-slide').stop(true, false).animate({
                height: 0
            }, 200, function () {
                $("#nav-slide,#nav-slider").hide();
            })
        });
        $('#nav-slide').delegate("li", "mouseover", function () {
            $(this).addClass("on")
        }).delegate("li", "mouseout", function () {
            $(this).removeClass("on")
        });
        $('#account').hover(function(){
            $(this).addClass('hover');
            $('#account-info').show();
        },function(){
            var t = $(this);
            $('#account-info').slideUp(200,function(){
                t.removeClass('hover');
            });
        });
    });
</script>
